  "componentMetadata": {
    "description": "A tracker container that assigns IDs to unique objects and skips detectiosn to track with keypoints.",
    "tags": [
      "alert"
    ],
    "license": "closed",
    "author": {
      "authorName": "poc",
      "authorEmail": "poc@ai.org",
      "authorLinks": {
        "linkName": "github",
        "url": "https://github.com/poc.org"
      }
    }
  },
  "modelDetails": {
    "id": "mdl-fastmot-tracker",
    "label": "Module",
    "name": "FastMOT Multi-Object Tracker",
    "category": "tracker",
    "framework": "PyTorch",
    "license": "closed",
    "repo_url": "https://github.com/poc.org"
  },
  "parameters": {
    "total_params_m": 2.1,
    "backbone": "ResNet50",
    "reid_model": "OSNet",
    "tracking_algorithm": "FastMOT",
    "kalman_filter": "enabled"
  },
  "training": {
    "pretrained": true,
    "input_resolution": [1920, 1080],
    "trained_on": {
      "dataset": "MOT Challenge + Custom Surveillance Data",
      "type": "multi_object_tracking",
      "scenarios": ["general", "crowd"],
      "notes": "Optimized for surveillance camera tracking with skip-frame detection"
    }
  },
  "runtime": {
    "gpu_required": true,
    "min_gpu_mem_gb": 4,
    "recommended_gpu": "NVIDIA T4",
    "cpu_cores": 4,
    "ram_gb": 8,
    "docker_image": "fastskiptrackerallres:latest",
    "env": {
      "python": "3.8",
      "cuda": "11.0",
      "cudnn": "8.0",
      "os": "ubuntu-20.04"
    }
  },
  "benchmarks": {
    "hardware": "NVIDIA T4",
    "test_conditions": {
      "input_resolution": [1920, 1080],
      "decoder": "TURBO",
      "interpolation": "INTERP_GAUSSIAN",
      "max_age": 9,
      "detector_skip_frame": 2,
      "precision": "FP32"
    },
    "tracking_performance": {
      "mota": 0.75,
      "motp": 0.82,
      "id_switches": "Low",
      "false_positives": "Minimal",
      "false_negatives": "Low"
    },
    "throughput_fps": {
      "1080p_general": 30,
      "1080p_crowd": 25,
      "720p_general": 45,
      "720p_crowd": 35
    },
    "gpu_utilization_percent": {
      "1080p_tracking": 65,
      "720p_tracking": 45
    },
    "gpu_memory_mb": {
      "1080p_tracking": 3200,
      "720p_tracking": 2400
    },
    "tracking_accuracy": {
      "id_consistency": 0.92,
      "occlusion_handling": 0.85,
      "re_identification": 0.88
    }
  },
  "produces": ["od1_list", "tracking_metadata"],
  "consumes": ["od1_list", "rgb_frames"],
  "input_formats": ["OD1"],
  "output_formats": ["OD1"],
  "sources": [
    {
      "title": "FastMOT: High-Performance Multiple Object Tracking",
      "url": "https://arxiv.org/abs/2105.14205"
    },
    {
      "title": "Internal tracking performance benchmarks",
      "url": "internal://benchmark-data/fastmot-tracker.txt"
    }
  ],
  "notes": "Multi-object tracker with re-identification capabilities. Supports skip-frame detection for performance optimization. Handles occlusions and crowded scenarios effectively. Configurable for general surveillance and crowd tracking scenarios. Uses Kalman filtering for motion prediction and OSNet for re-identification."
}
