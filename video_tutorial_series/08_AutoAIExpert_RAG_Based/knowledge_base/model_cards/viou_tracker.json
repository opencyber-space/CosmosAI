{
    "componentId": {
        "name": "trackerlite",
        "version": "v0.0.1",
        "release": "stable"
    },
    "componentType": "node.algorithm.tracker",
    "containerImage": "vioutrackerdocker:latest",
    "requiresGPU": true,
    "componentConfig": {
        "requireFrames": true,
        "frameSize": [
            {
                "width": 1920,
                "height": 1080
            }
        ],
        "supportsBatching": true,
        "batchSize": 4,
        "settings": [
            {
                "name": "ttl",
                "type": "int",
                "default": 4
            },
            {
                "name": "tracker_type",
                "type": "String",
                "default": "MEDIANFLOW",
                "options": ["KCF","MEDIANFLOW","NONE"]
            },
            {
                "name": "keep_upper_height_ratio",
                "type": "float",
                "min": 0.0,
                "max": 1.0,
                "default": 1.0

            },
            {
                "name": "decoderType",
                "type": "String",
                "default": "DALI",
                "options": ["TURBO","DALI"]
            },
            {
                "name": "interpolationType",
                "type": "String",
                "default": "INTERP_GAUSSIAN",
                "options": ["INTERP_NN","INTERP_LINEAR","INTERP_CUBIC","INTERP_TRIANGULAR","INTERP_GAUSSIAN","INTERP_LANCZOS3"]
            },
            {
                "name": "enable_batching",
                "type": "bool",
                "default": true
            },
            {
                "name": "batch_size",
                "type": "int",
                "min": 1,
                "max": 16,
                "default": 4
            }
            
        ],
        "parameters": [
            {
                "name": "sigma_iou",
                "type": "float",
                "min": 0,
                "max": 1.0,
                "default": 0.3
            },
            {
                "name": "sigma_h",
                "type": "float",
                "min": 0,
                "max": 1.0,
                "default": 0.3
            },
            {
                "name": "sigma_l",
                "type": "float",
                "min": 0,
                "max": 1.0,
                "default": 0.1
            },
            {
                "name": "t_min",
                "type": "int",
                "default": 4,
                "min": -1,
                "max": 8
            }
        ],
        "inputs": [
            "input_0"
        ],
        "outputs": [
            "output_0"
        ]
    },
    "componentMetadata": {
        "description": "V-IOU tracker container for lightweight multi-object tracking using intersection-over-union metrics",
        "tags": [
           "tracking",
           "viou",
           "lightweight"
        ],
        "license": "closed",
        "author": {
            "authorName": "poc",
            "authorEmail": "poc@ai.org",
            "authorLinks": {
                "linkName": "github",
                "url": "https://github.com/poc.org"
            }
        }
    },
    "modelDetails": {
        "id": "mdl-viou-tracker",
        "label": "Module", 
        "name": "V-IOU Tracker Lite",
        "category": "tracker",
        "framework": "OpenCV",
        "license": "closed",
        "repo_url": "https://github.com/poc.org"
    },
    "parameters": {
        "total_params_m": 0.1,
        "tracking_algorithm": "V-IOU",
        "association_method": "IoU + Visual",
        "kalman_filter": "optional",
        "memory_footprint": "minimal"
    },
    "training": {
        "pretrained": false,
        "input_resolution": [1920, 1080],
        "trained_on": {
            "dataset": "Rule-based algorithm",
            "type": "intersection_over_union_tracking",
            "scenarios": ["general", "lightweight", "embedded"],
            "notes": "Non-learning based tracker using geometric and visual cues"
        }
    },
    "runtime": {
        "gpu_required": true,
        "min_gpu_mem_gb": 1,
        "recommended_gpu": "NVIDIA T4",
        "cpu_cores": 2,
        "ram_gb": 2,
        "docker_image": "vioutrackerdocker:latest",
        "env": {
            "python": "3.8",
            "opencv": "4.5",
            "cuda": "11.0",
            "os": "ubuntu-20.04"
        }
    },
    "benchmarks": {
        "hardware": "NVIDIA T4",
        "test_conditions": {
            "input_resolution": [1920, 1080],
            "decoder": "DALI",
            "interpolation": "INTERP_GAUSSIAN",
            "batch_size": 4,
            "tracker_type": "MEDIANFLOW"
        },
        "tracking_performance": {
            "mota": 0.68,
            "motp": 0.76,
            "id_switches": "Medium",
            "false_positives": "Low",
            "false_negatives": "Medium"
        },
        "throughput_fps": {
            "1080p_general": 60,
            "720p_general": 85,
            "480p_general": 120
        },
        "gpu_utilization_percent": {
            "1080p_tracking": 25,
            "720p_tracking": 18,
            "480p_tracking": 12
        },
        "gpu_memory_mb": {
            "1080p_tracking": 800,
            "720p_tracking": 600,
            "480p_tracking": 400
        },
        "tracking_accuracy": {
            "id_consistency": 0.78,
            "occlusion_handling": 0.65,
            "re_identification": 0.70
        }
    },
    "produces": ["od1_list", "tracking_metadata"],
    "consumes": ["od1_list", "rgb_frames"],
    "input_formats": ["OD1"],
    "output_formats": ["OD1"],
    "sources": [
        {
            "title": "V-IOU: Similarity Measures for Accurate Visual Tracking",
            "url": "https://arxiv.org/abs/1611.05971"
        },
        {
            "title": "Internal V-IOU tracker performance benchmarks",
            "url": "internal://benchmark-data/viou-tracker.txt"
        }
    ],
    "notes": "Lightweight visual tracker based on intersection-over-union metrics. Designed for resource-constrained environments. Good balance between speed and accuracy for simple tracking scenarios. Supports multiple tracker types including KCF and MEDIANFLOW."
}
