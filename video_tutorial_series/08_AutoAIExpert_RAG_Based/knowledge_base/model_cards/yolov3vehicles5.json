    },
    "parameters": {
        "total_params_m": 61.9,
        "backbone": "Darknet-53",
        "neck": "FPN",
        "head": "YOLOv3 Head",
        "anchor_config": "[[10,13],[16,30],[33,23],[30,61],[62,45],[59,119],[116,90],[156,198],[373,326]]"
