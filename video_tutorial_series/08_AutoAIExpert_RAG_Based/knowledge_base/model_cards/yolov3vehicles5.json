        "pretrained": true,
        "input_resolution": [416, 416],
        "trained_on": {
            "dataset": "Custom Vehicles Dataset for surveillance applications",
            "classes": [
                "car",
                "bus", 
                "truck",
                "motorbike",
                "autorickshaw"
            ],
            "type": "vehicle_detection",
            "categories": 5,
            "notes": "Optimized for vehicle detection in surveillance scenarios with 5 vehicle categories"
        }
    },
    "runtime": {
        "gpu_required": true,
        "min_gpu_mem_gb": 2,
        "recommended_gpu": "NVIDIA T4",
        "cpu_cores": 4,
        "ram_gb": 8,
        "docker_image": "yolov3vehicles5docker:latest",
        "env": {
            "python": "3.8",
            "cuda": "11.0",
            "cudnn": "8.0",
            "os": "ubuntu-20.04"
        }
    },
    "benchmarks": {
        "hardware": "NVIDIA T4",
        "test_conditions": {
            "input_resolution": [416, 416],
            "decoder": "DALI",
