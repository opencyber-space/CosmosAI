        "optimal_batch_size": {
            "performance": 16,
            "efficiency": 8,
            "memory_constrained": 4
        }
    },
    "produces": ["vehicle_bboxes", "vehicle_classifications"],
    "consumes": ["rgb_frames"],
    "input_formats": ["OD1"],
    "output_formats": ["OD1"],
    "sources": [
        {
            "title": "YOLOv3: An Incremental Improvement",
